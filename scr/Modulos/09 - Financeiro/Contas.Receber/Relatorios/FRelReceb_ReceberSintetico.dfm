  object db_CtaReceber: TUniQuery
    Connection = FrmPrincipal.DBConexao
    SQL.Strings = (
      ''
      'select  '
      'nome_cliente,'
      'sum(valor) as valor_saldo'
      ''
      'from view_fin_ctareceber'
      'where'
      '(VENCIMENTO>=:data1 AND VENCIMENTO<=:data2)'
      'and'
      'quitado<>'#39'S'#39
      'group by  nome_cliente'
      ''
      'order by nome_cliente'
      ''
      ''
      '')
